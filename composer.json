{
  "name": "mezzolabs/mezzo",
  "description": "Mezzo - mix your content with a sexy backend.",
  "license": "proprietary",
  "keywords": [
    "laravel",
    "mezzo",
    "cms",
    "cmf",
    "admin",
    "acp",
    "generator"
  ],
  "homepage": "https://github.com/MezzoLabs/Mezzo",
  "authors": [
    {
      "name": "Simon Schneider, Marc Vornetran",
      "email": "mail@mezzolabs.io",
      "homepage": "http://mezzolabs.io",
      "role": "Developers"
    }
  ],
  "require": {
    "illuminate/support": "~5.2",
    "php": ">=7.0.0",
    "dingo/api": "~1.0",
    "tymon/jwt-auth": "0.5.*",
    "doctrine/dbal": "^2.5",
    "laravelcollective/html": "~5.0",
    "codesleeve/laravel-stapler": "1.0.*",
    "kalnoy/nestedset": "3.*",
    "league/glide": "^1.0",
    "spatie/laravel-glide": "3.*",
    "felixkiss/uniquewith-validator": "2.*",
    "cviebrock/eloquent-sluggable": "dev-master",
    "caouecs/laravel4-lang": "~2.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "skovmand/mailchimp-laravel": "1.*",
    "laravel/socialite": "^2.0",
    "predis/predis": "~1.0",
    "webpatser/laravel-countries": "dev-master",
    "lavary/laravel-menu": "dev-master",
    "davejamesmiller/laravel-breadcrumbs": "dev-master",
    "raven/raven": "^0.13.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1",
    "barryvdh/laravel-ide-helper": "^2.0@dev",
    "laracasts/generators": "dev-master",
    "bugsnag/bugsnag-laravel": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Mezzolabs\\Mezzo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzoLabs\\Mezzo\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
